{
  "expo": {
    "name": "zipbap",
    "owner": "zipbap",
    "slug": "zipbap",
    "scheme": "zipbap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img/slash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#DA5534"
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.zipbap",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "레시피 사진을 업로드하기 위해 사진 라이브러리에 접근합니다.",
        "NSCameraUsageDescription": "레시피 사진을 촬영하기 위해 카메라에 접근합니다.",
        "NSMicrophoneUsageDescription": "영상 녹화를 위해 마이크에 접근합니다.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "12"
    },
    "plugins": ["expo-apple-authentication", "expo-video"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zipbap"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/img/app-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c84832a3-9f25-48ab-947e-9136a4a2fa97"
      }
    }
  }
}
