{
  "expo": {
    "name": "zipbap",
    "slug": "zipbap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img/slash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#DA5534"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kgs9843.zipbap"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kgs9843.zipbap"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/img/app-icon.png"
    }
  }
}
